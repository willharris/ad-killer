{
    "name": "Kill Ads",
    "version": "1.0.1",
    "description": "Chrome extension to remove all ads from pages on certain websites",
    "manifest_version": 2,
    "background" : {
        "scripts" : ["background.js"],
        "persistent": false
    },
    "icons": {
        "256": "favicon.png"
    },
    "browser_action": {
        "default_icon": "favicon.png"
    },
    "permissions": ["activeTab", "tabs"]
  }