{
    "name": "Kill WP Ads",
    "version": "1.0",
    "description": "Removes all ads from a Washington Post page",
    "manifest_version": 2,
    "background" : {
        "scripts" : ["background.js"],
        "persistent": false
    },
    "icons": {
        "256": "favicon.png"
    },
    "browser_action": {
        "default_icon": "favicon.png"
    },
    "permissions": ["activeTab"]
  }